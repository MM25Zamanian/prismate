{
  "name": "@prismate/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Core foundation module for Prismate with advanced type safety and performance optimizations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "benchmark": "tsx src/benchmarks/index.ts",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@prismate/eslint-config": "workspace:*",
    "@prismate/typescript-config": "workspace:*",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@prisma/client": "^5.0.0"
  },
  "keywords": [
    "prisma",
    "orm",
    "typescript",
    "validation",
    "database",
    "core",
    "types",
    "utilities"
  ],
  "author": "Prismate Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}